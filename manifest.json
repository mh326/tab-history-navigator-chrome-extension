{
    "manifest_version": 3,
    "name": "Tab History Navigator",
    "version": "1.0",
    "description": "Navigate through tab history with keyboard shortcuts",
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "go-back-tab": {
            "suggested_key": {
                "default": "Ctrl+Shift+Left"
            },
            "description": "Go back tab in history"
        },
        "go-forward-tab": {
            "suggested_key": {
                "default": "Ctrl+Shift+Right"
            },
            "description": "Go forward tab in history"
        }
    }
}
